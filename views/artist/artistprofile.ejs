<aside>
    <h5>Account Details</h5>
    <p><%=artist.user[0].firstName%> </p>
    <p><%=artist.user[0].lastName%> </p>
    <p><%=artist.user[0].emailAddress%> </p>
</aside>

<section id="artistPage">
    <img  id="imgartistPage" src="../image/<%=artist.user[0].image%>">
    <div>
    <a href="/artist/delete?id=<%=artist._id%>"> Delete Artist Profile</a>
    <a href="/artist/edit?id=<%=artist._id%>"> Edit Account</a>
</div>

    </div>
    <div><%=artist.bandName%></div>
    <div><%=artist.bio%></div>
    <% for(let i=0 ;i< artist.genres.length; i++){%>
    <div>
     #<%=artist.genres[i]%>
    
     <% } %>
    </div>
    
    
    <div class="grid-container">
    
        <%artist.event.forEach(function(event) { %>
    <div class="grid-section">
       <img src="../image/<%=event.eventPhoto%>">
        <h3><%=event.name%></h3>
        <p><%=event.venue%></p>
        <p><%=event.city%></p>
        <p><%=event.date%></p>
        <p><a href="/event/detail?id=<%=event._id%>">View Event</a></p>
        <a href="/event/delete?id=<%=event._id%>"> Delete Event</a>
    
    </div>
    <% }) %>
    </div>
    </section>
    